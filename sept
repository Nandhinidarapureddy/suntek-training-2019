#include <stdio.h>
int main()
{
    int arr[100],n,i;
    int num,count;
     printf("Enter total number of elements: ");
    scanf("%d",&n);
      printf("Enter array elements:\n");
    for(i=0;i< n;i++)
    {
        scanf("%d",&arr[i]);
    }
      printf("count:");
      scanf("%d",&num);
      count=0;
    for(i=0;i< n;i++)
    {
        if(arr[i]==num)
            count++;
            else
            return -1;
            
    }
    printf(" :%d\n",count);
    return 0;
}
