#include <stdio.h> 
  

void print_squaredi(int n) 
{ 
    int i, j; 
    for (i = 1; i <= n; i++) 
    { 
        for (j = 1; j <= n; j++) 
        { 
            if (i==1 || i==n || j==1 ||  
                j==n || i==j || j==(n - i + 1))             
                printf("*");             
            else            
                printf(" ");             
        } 
        printf("\n"); 
    } 
  
} 
// Driver code for above function 
int main() 
{ 
    int rows = 8; 
    print_squaredi(rows); 
    return 0; 
} 
